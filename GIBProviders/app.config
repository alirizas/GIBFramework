<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IIntegration">
                    <security mode="TransportWithMessageCredential" />
                </binding>
                <binding name="PostBoxServiceEndpoint">
                    <security mode="Transport" />
                </binding>
                <binding name="PostBoxServiceEndpoint1" />
            </basicHttpBinding>
            <customBinding>
                <binding name="CustomBinding_ITransferDocumentService">
                    <mtomMessageEncoding messageVersion="Soap12" />
                    <httpTransport />
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="http://transfertest.veriban.com.tr/TransferDocumentService.svc"
                binding="customBinding" bindingConfiguration="CustomBinding_ITransferDocumentService"
                contract="ServiceVeriban.ITransferDocumentService" name="CustomBinding_ITransferDocumentService" />
            <endpoint address="https://efatura-test.uyumsoft.com.tr/Services/Integration"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IIntegration"
                contract="ServiceUyumsoft.IIntegration" name="BasicHttpBinding_IIntegration" />
            <endpoint address="https://pb.diyalogo.com.tr/PostBoxService.svc"
                binding="basicHttpBinding" bindingConfiguration="PostBoxServiceEndpoint"
                contract="ServiceLogo.IPostBoxService" name="PostBoxServiceEndpoint" />
            <endpoint address="http://pb.elogo.com.tr/PostBoxService.svc"
                binding="basicHttpBinding" bindingConfiguration="PostBoxServiceEndpoint1"
                contract="ServiceLogo.IPostBoxService" name="PostBoxServiceEndpoint1" />
        </client>
    </system.serviceModel>
</configuration>